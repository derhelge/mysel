# default Konfiguration aus Auslieferungszustand
# docker compose -f docker/dev/docker-compose.dev.yml exec freeradius cat /etc/raddb/mods-available/eap > docker/dev/freeradius/raddb/mods-available/eap
# grep -v -E '^[[:space:]]*(#|$)' docker/dev/freeradius/raddb/mods-available/eap > docker/dev/freeradius/raddb/mods-enabled/eap
eap eap-tfh-bochum-de {
	default_eap_type = tls
	timer_expire = 60
	ignore_unknown_eap_types = no
	cisco_accounting_username_bug = no
	max_sessions = ${max_requests}

	tls-config tls-tfh-bochum-de {
		private_key_file = /etc/raddb/certs/radius.tfh-bochum.de-20250714.key
		certificate_file = /etc/raddb/certs/radius.tfh-bochum.de-20250714.pem
		ca_file = /etc/raddb/certs/ca-tfh-bochum.de.pem
		fragment_size = 1024
		ocsp {
				enable = no
		}
	}
	tls {
		tls = tls-tfh-bochum-de
	}
}
